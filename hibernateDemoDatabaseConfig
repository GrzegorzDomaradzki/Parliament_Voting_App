-- Table: public.test_hibernate

-- DROP TABLE public.test_hibernate;

CREATE TABLE public.test_hibernate
(
  id integer NOT NULL,
  name character varying(13),
  some_number integer,
  CONSTRAINT test_hibernate_pkey PRIMARY KEY (id)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE public.test_hibernate
  OWNER TO sql_client;
GRANT ALL ON TABLE public.test_hibernate TO sql_client;



-- Sequence: public.test_hibernate_id_seq

-- DROP SEQUENCE public.test_hibernate_id_seq;

CREATE SEQUENCE public.test_hibernate_id_seq
  INCREMENT 1
  MINVALUE 1
  MAXVALUE 9223372036854775807
  START 2
  CACHE 1;
ALTER TABLE public.test_hibernate_id_seq
  OWNER TO sql_client;



-- Sequence: public.hibernate_sequence

-- DROP SEQUENCE public.hibernate_sequence;

CREATE SEQUENCE public.hibernate_sequence
  INCREMENT 1
  MINVALUE 1
  MAXVALUE 9223372036854775807
  START 7
  CACHE 1;
ALTER TABLE public.hibernate_sequence
  OWNER TO postgres;
GRANT ALL ON SEQUENCE public.hibernate_sequence TO postgres;
GRANT ALL ON SEQUENCE public.hibernate_sequence TO sql_client;

